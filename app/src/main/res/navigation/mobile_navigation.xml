<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/nav_dashboard">

    <fragment
        android:id="@+id/nav_dashboard"
        android:name="io.bewsys.spmobile.ui.dashboard.DashboardFragment"
        android:label="Dashboard"
        tools:layout="@layout/fragment_dashboard" />
    <fragment
        android:id="@+id/nav_household"
        android:name="io.bewsys.spmobile.ui.households.HouseholdsFragment"
        android:label="Households"
        tools:layout="@layout/fragment_households" >
        <action
            android:id="@+id/action_nav_household_to_humanitarianFormFragment"
            app:destination="@id/humanitarianFormFragment" />
        <action
            android:id="@+id/action_nav_household_to_navigation"
            app:destination="@id/form_navigation" />
    </fragment>
    <fragment
        android:id="@+id/nav_non_consenting"
        android:name="io.bewsys.spmobile.ui.nonconsenting.NonConsentingFragment"
        android:label="Non-consenting Households"
        tools:layout="@layout/fragment_non_consenting" >
        <action
            android:id="@+id/action_nav_non_consenting_to_nonConsentingFormFragment"
            app:destination="@id/nonConsentingFormFragment" />
    </fragment>
    <fragment
        android:id="@+id/nav_profile"
        android:name="io.bewsys.spmobile.ui.profile.ProfileFragment"
        android:label="Profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/nonConsentingFormFragment"
        android:name="io.bewsys.spmobile.ui.nonconsenting.form.AddNonConsentingHouseholdFragment"
        android:label="Add non-consenting household"
        tools:layout="@layout/fragment_add_non_consenting" />
    <fragment
        android:id="@+id/humanitarianFormFragment"
        android:name="io.bewsys.spmobile.ui.households.forms.HumanitarianFormFragment"
        android:label="Add humanitarian registration"
        tools:layout="@layout/fragment_humanitarian_form" />
    <fragment
        android:id="@+id/nav_settings"
        android:name="io.bewsys.spmobile.ui.settings.SettingsFragment"
        android:label="Settings" >
        <action
            android:id="@+id/action_nav_settings_to_hostSettingsFragment"
            app:destination="@id/hostSettingsFragment" />
    </fragment>
    <fragment
        android:id="@+id/hostSettingsFragment"
        android:name="io.bewsys.spmobile.ui.settings.HostSettingsFragment"
        android:label="Host settings" />
    <fragment
        android:id="@+id/nav_login"
        android:name="io.bewsys.spmobile.ui.auth.LoginFragment"
        android:label="Login"
        tools:layout="@layout/fragment_login" />
    <dialog
        android:id="@+id/loginDialogFragment"
        android:name="io.bewsys.spmobile.ui.auth.LoginDialogFragment"
        android:label="LoginDialogFragment" >
        <argument
            android:name="errorMessage"
            app:argType="string"
            android:defaultValue="Login failed!" />
    </dialog>
    <action android:id="@+id/action_global_loginDialogFragment" app:destination="@id/loginDialogFragment" />
    <navigation android:id="@+id/form_navigation"
        app:startDestination="@id/formStepOneFragment">
        <fragment
            android:id="@+id/formStepTwoFragment"
            android:name="io.bewsys.spmobile.ui.households.forms.developmentalform.FormStepTwoFragment"
            android:label="fragment_add_household_two_head"
            tools:layout="@layout/fragment_add_household_two_respondent">
            <action
                android:id="@+id/action_formStepTwoFragment_to_formStepThreeFragment"
                app:destination="@id/formStepThreeFragment" />
            <action
                android:id="@+id/action_formStepTwoFragment_to_formStepOneFragment"
                app:destination="@id/formStepOneFragment" />
        </fragment>
        <fragment
            android:id="@+id/formStepOneFragment"
            android:name="io.bewsys.spmobile.ui.households.forms.developmentalform.FormStepOneFragment"
            android:label="fragment_add_household_one_respondent"
            tools:layout="@layout/fragment_add_household_one_consent">
            <action
                android:id="@+id/action_formStepOneFragment_to_formStepTwoFragment"
                app:destination="@id/formStepTwoFragment" />
        </fragment>
        <fragment
            android:id="@+id/formStepFiveFragment"
            android:name="io.bewsys.spmobile.ui.households.forms.developmentalform.FormStepFiveFragment"
            android:label="fragment_add_household_five_property"
            tools:layout="@layout/fragment_add_household_five_shock">
            <action
                android:id="@+id/action_formStepFiveFragment_to_formStepFourFragment"
                app:destination="@id/formStepFourFragment" />
            <action
                android:id="@+id/action_formStepFiveFragment_to_formStepSixFragment2"
                app:destination="@id/formStepSixFragment2" />
        </fragment>
        <fragment
            android:id="@+id/formStepThreeFragment"
            android:name="io.bewsys.spmobile.ui.households.forms.developmentalform.FormStepThreeFragment"
            android:label="fragment_add_household_three_household"
            tools:layout="@layout/fragment_add_household_three_head">
            <action
                android:id="@+id/action_formStepThreeFragment_to_formStepFourFragment"
                app:destination="@id/formStepFourFragment" />
            <action
                android:id="@+id/action_formStepThreeFragment_to_formStepTwoFragment"
                app:destination="@id/formStepTwoFragment" />
        </fragment>
        <fragment
            android:id="@+id/formStepFourFragment"
            android:name="io.bewsys.spmobile.ui.households.forms.developmentalform.FormStepFourFragment"
            android:label="fragment_add_household_four_home"
            tools:layout="@layout/fragment_add_household_four_home">
            <action
                android:id="@+id/action_formStepFourFragment_to_formStepFiveFragment"
                app:destination="@id/formStepFiveFragment" />
            <action
                android:id="@+id/action_formStepFourFragment_to_formStepThreeFragment"
                app:destination="@id/formStepThreeFragment" />
        </fragment>
        <fragment
            android:id="@+id/formStepSixFragment2"
            android:name="io.bewsys.spmobile.ui.households.forms.developmentalform.FormStepSixFragment"
            android:label="FormStepSixFragment" >
            <action
                android:id="@+id/action_formStepSixFragment2_to_formStepFiveFragment"
                app:destination="@id/formStepFiveFragment" />
        </fragment>
    </navigation>
</navigation>