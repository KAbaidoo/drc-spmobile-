getAllHouseholds:
SELECT *
FROM householdEntity;


getHouseholdCount:
SELECT COUNT(*)
FROM householdEntity;

insertHousehold:
INSERT OR REPLACE
INTO householdEntity
VALUES(?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?);

getByRemoteId:
SELECT *
FROM householdEntity
WHERE remote_id = :remote_id;

updateStatus:
UPDATE householdEntity
SET status = :status WHERE id = :id;

getById:
SELECT *
FROM householdEntity
WHERE id = :id;

getByCommunityId:
SELECT *
FROM householdEntity
WHERE community_id = :community_id;

getByProvinceId:
SELECT *
FROM householdEntity
WHERE province_id = :province_id;


lastInsertRowId:
SELECT last_insert_rowid();




