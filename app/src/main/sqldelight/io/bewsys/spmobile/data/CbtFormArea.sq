CREATE TABLE cbtFormAreas (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    cbt_form_id INTEGER,
    assigned_to_type TEXT ,
    assigned_to INTEGER ,
    status TEXT
);

getAllCbtFormAreas:
SELECT *
FROM cbtFormAreas;

insertCbtFormArea:
INSERT
INTO cbtFormAreas
VALUES(?,?,?,?,?);

getByCbtFormId:
SELECT *
FROM cbtFormAreas
WHERE cbt_form_id = :cbt_form_id;






