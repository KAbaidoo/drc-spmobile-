CREATE TABLE healthAreaEntity (
    id INTEGER NOT NULL PRIMARY KEY,
    name TEXT,
    health_zone_id TEXT
);

getByHealthZoneId:
SELECT *
FROM healthAreaEntity
WHERE health_zone_id = :health_zone_id;

getAllHealthAreas:
SELECT *
FROM healthAreaEntity;



insertHealthArea:
INSERT OR REPLACE
INTO healthAreaEntity
VALUES(?,?,?);

getById:
SELECT *
FROM healthAreaEntity
WHERE id = :id;